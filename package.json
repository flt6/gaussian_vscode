{
  "name": "gaussian_input",
  "displayName": "Gaussian Input File Syntax and Completion",
  "description": "Syntax highlighting and code completion for Gaussian input files",
  "version": "1.0.0",
  "publisher": "flt",
  "repository": "https://github.com/flt6/gaussian_vscode",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:GaussianInput"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "GaussianInput",
        "aliases": [
          "Gaussian Input File",
          "GaussianInput"
        ],
        "extensions": [
          ".gin",
          ".gjf",
          ".com"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "GaussianInput",
        "scopeName": "source.gaussian",
        "path": "./syntaxes/GaussianInput.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "GaussianInput",
        "path": "./snippets/gaussian.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.x",
    "@types/vscode": "^1.51.0",
    "typescript": "^4.9.4"
  }
}
